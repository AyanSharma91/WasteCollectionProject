<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_pressed="true">
        <shape>
            <solid
                android:color="#F63F06"/>
            <stroke
                android:color="#000"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#BFFB835D"/>
            <stroke
                android:color="#BFFB835D"
                android:width="1dp"/>
            <corners
                android:radius="50dp"/>
        </shape>
    </item>


</selector>